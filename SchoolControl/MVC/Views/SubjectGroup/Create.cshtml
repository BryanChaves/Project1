            @Html.EditorFor(model => model.idSubjectGroup)
            @Html.ValidationMessageFor(model => model.idSubjectGroup)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.idGroup, "Group")
        </div>
        <div class="editor-field">
            @Html.DropDownList("idGroup", String.Empty)
