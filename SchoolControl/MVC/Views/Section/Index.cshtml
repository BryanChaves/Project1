@model IEnumerable<Data.Section>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Section1)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Section1)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { Section = item.Section1 }) |
                @Html.ActionLink("Details", "Details", new { Section = item.Section1 }) |
                @Html.ActionLink("Delete", "Delete", new { Section = item.Section1 })
            </td>
        </tr>
    }

</table>
