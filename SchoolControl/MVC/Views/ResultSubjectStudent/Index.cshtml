@model IEnumerable<Data.ResultSubjectStudent>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Student1.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject1.Subject1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Result1.Result1)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Student1.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject1.Subject1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result1.Result1)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
